@inject IResourceManager _resourceManager
@model ApplicationUserInfoDto
@{
    ViewData["Title"] = _resourceManager[SharedResource.Login];
}
<div class="breadcrumbs_area">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="breadcrumb_content">
                    <ul>
                        <li><a href="/">@_resourceManager[SharedResource.HomePage]</a></li>
                        <li>@_resourceManager[SharedResource.Panel]</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>

<section class="main_content_area">
    <div class="container">
        <div class="account_dashboard">
            <div class="row">
                <partial name="_sidebar" />
            </div>
        </div>
    </div>
</section>

@section Scripts{
    <script>
        $(document).ready(function () {
            $("a").removeClass("active");
            $("#indexUser").addClass("active");
            
        });
    </script>
}